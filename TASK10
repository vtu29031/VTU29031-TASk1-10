
facts = [
    "john_is_cold.",
    "raining.",
    "john_Forgot_His_Raincoat.",
    "fred_lost_his_car_keys.",
    "peter_footballer."
]

def verify_fact(fact):
    fact = fact.rstrip(".")
    if fact in ["john_Forgot_His_Raincoat", "raining", "foggy"]:
        return True
    return False

for fact in facts:
    if verify_fact(fact):
        print(f"{fact} - Yes")
    else:
        print(f"{fact} - No")
john_is_cold. - No
raining. - Yes
john_Forgot_His_Raincoat. - Yes
fred_lost_his_car_keys. - No
peter_footballer. - No
 
